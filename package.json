{
  "name": "filler-sites",
  "version": "1.0.0",
  "description": "Автоматическое заполнение формы заявки на электронную визу Вьетнама",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:evisa": "playwright test tests/evisa-vietnam.spec.ts",
    "test:evisa:headed": "playwright test tests/evisa-vietnam.spec.ts --headed",
    "test:evisa-login": "playwright test tests/evisa-vietnam-login.spec.ts",
    "test:evisa-login:headed": "playwright test tests/evisa-vietnam-login.spec.ts --headed",
    "test:evisa-login:debug": "playwright test tests/evisa-vietnam-login.spec.ts --debug",
    "test:evisa-simple": "playwright test tests/evisa-vietnam-login-simple.spec.ts",
    "test:evisa-simple:headed": "playwright test tests/evisa-vietnam-login-simple.spec.ts --headed",
    "test:evisa-simple:debug": "playwright test tests/evisa-vietnam-login-simple.spec.ts --debug",
    "test:evisa-fixed": "playwright test tests/evisa-vietnam-fixed.spec.ts",
    "test:evisa-fixed:headed": "playwright test tests/evisa-vietnam-fixed.spec.ts --headed",
    "test:evisa-fixed:debug": "playwright test tests/evisa-vietnam-fixed.spec.ts --debug",
    "test:debug": "playwright test tests/evisa-vietnam-debug.spec.ts --headed",
    "test:debug:ui": "playwright test tests/evisa-vietnam-debug.spec.ts --ui",
    "test:multi": "playwright test tests/multi-user.spec.ts",
    
    "test:full-flow": "playwright test tests/evisa-vietnam-full-flow.spec.ts --headed",
    "test:full-flow:ui": "playwright test tests/evisa-vietnam-full-flow.spec.ts --ui",
    "test:full-flow:headed": "playwright test tests/evisa-vietnam-full-flow.spec.ts --headed --workers=1",
    "test:multi:headed": "playwright test tests/multi-user.spec.ts --headed",
    "report": "playwright show-report",
    "install:browsers": "playwright install",
    "user:list": "node scripts/userManager.js list",
    "user:status": "node scripts/userManager.js status",
    "user:switch": "node scripts/userManager.js switch",
    "user:create": "node scripts/userManager.js create",
    "user:check": "node scripts/userManager.js check",
    "user:help": "node scripts/userManager.js help",
    "build": "tsc --noEmit",
    "lint": "npx tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lindon0390/filler-sites.git"
  },
  "keywords": [
    "playwright",
    "automation",
    "evisa",
    "vietnam",
    "form-filling",
    "multi-user"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lindon0390/filler-sites/issues"
  },
  "homepage": "https://github.com/lindon0390/filler-sites#readme",
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@types/node": "^20.14.2",
    "typescript": "^5.8.3"
  }
}
